<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.Error {
			color:RED;
			size:14px;
		}
	</ui:style>
	<g:HTMLPanel>
	    <g:VerticalPanel>
	        <g:Label ui:field="gammeltLabel" text="Indtast dit gamle password" />
	        <g:PasswordTextBox ui:field="gammeltPassword" />
	        <g:Label ui:field="nytLabel" text="Indtast dit nye password" />
	        <g:PasswordTextBox ui:field="nytPassword" />
	        <g:Label ui:field="gentagLabel" text="Gentag dit nye password" />
	        <g:PasswordTextBox ui:field="gentagPassword" />
	        <g:Label ui:field="errorLabel" styleName="{style.Error}"/>
	        <g:Button ui:field="submit" text="Skift password" />
	    </g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 