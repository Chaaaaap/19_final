<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.Labels {
			font-size:18px;
			width:150px;
			text-align:center;
		}

		.Rediger {
			margin-top:0;
		}
	</ui:style>
	<g:HTMLPanel>
	    <g:Button ui:field="visListe" text="Vis liste" />
	    <g:HorizontalPanel>
	        <g:Label ui:field="oprID" text="Operatør ID" styleName="{style.Labels}"/>
	        <g:Label ui:field="oprNavn" text="Operatør navn" styleName="{style.Labels}"/>
	        <g:Label ui:field="ini" text="Initialer" styleName="{style.Labels}"/>
	        <g:Label ui:field="cpr" text="CPR-nummer" styleName="{style.Labels}"/>
	        <g:Label ui:field="password" text="Password" styleName="{style.Labels}"/>
	    </g:HorizontalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 