<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.Labels {
			font-size:18px;
			width:150px;
			text-align:center;
		}

		.Rediger {
			margin-top:0;
		}

		.ListBox {
			height:23px;
			padding-top:3px;
		}

		.Error {
			color:RED;
			size:14px;
		}
	</ui:style>
	<g:HTMLPanel>
	    <g:HorizontalPanel>
	        <g:Label ui:field="oprID" styleName="{style.Labels}"/>
	        <g:Label ui:field="oprNavn" styleName="{style.Labels}"/>
	        <g:Label ui:field="ini" styleName="{style.Labels}"/>
	        <g:Label ui:field="cpr" styleName="{style.Labels}"/>
	        <g:Label ui:field="password" styleName="{style.Labels}"/>
	        <g:Label ui:field="type" styleName="{style.Labels}"/>
	    </g:HorizontalPanel>
	    <g:Label ui:field="errorLabel" styleName="{style.Error}" />
	</g:HTMLPanel>
</ui:UiBinder> 